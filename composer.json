{
  "name": "allansun/openapi-runtime",
  "description": "Runtime library to be used with other SDK generated from OpenAPI docs",
  "license": "MIT",
  "keywords": [
    "openapi",
    "swagger",
    "kubernetes",
    "docker",
    "runtime"
  ],
  "authors": [
    {
      "name": "Allan Sun",
      "email": "allan.sun@bricre.com"
    }
  ],
  "require": {
    "php": "~7.4|^8.0",
    "ext-json": "*",
    "symfony/property-info": "^4|^5",
    "phpdocumentor/reflection-docblock": "^5.0",
    "psr/http-client": "^1.0.1",
    "psr/log": "^1.1.3",
    "symfony/http-client-contracts": "^v2.3.1"
  },
  "require-dev": {
    "phpunit/phpunit": "~9",
    "phpspec/prophecy": "^1.12",
    "phpspec/prophecy-phpunit": "^2.0",
    "symfony/http-client": "^v5.2.4",
    "guzzlehttp/guzzle": "^7"
  },
  "suggest": {
    "monolog/monolog": "^2",
    "symfony/http-client": "^v5.2.6",
    "guzzlehttp/guzzle": "^7"
  },
  "autoload": {
    "psr-4": {
      "OpenAPI\\Runtime\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "OpenAPI\\Runtime\\Tests\\": "tests"
    }
  },
  "prefer-stable": true
}
